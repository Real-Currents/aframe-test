{
  "name": "amplify-ui-demo-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "@aws-amplify/backend-cli": "^0.9.2",
    "@aws-amplify/core": "^6.0.7",
    "@aws-amplify/ui": "^6.0.5",
    "@aws-amplify/ui-react": "^6.0.6",
    "@aws-amplify/ui-react-core": "^3.0.5",
    "@cori-risi/cori.data.api": "github:ruralinnovation/cori.data.api",
    "@guanghechen/rollup-plugin-copy": "^6.0.0-alpha.6",
    "aws-amplify": "^6.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "xstate": "^4.38.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/modules/*.js",
      "!src/plugins/*.js",
      "!src/renv/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "build",
      "dist",
      "node_modules",
      "modules",
      "renv",
      "/build/",
      "/dist/",
      "/node_modules/",
      "/modules/",
      "/renv/",
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "testPathIgnorePatterns": [
      "build",
      "dist",
      "node_modules",
      "modules",
      "renv",
      "/build/",
      "/dist/",
      "/node_modules/",
      "/modules/",
      "/renv/",
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "verbose": true
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  }
}
