{
  "name": "frontend",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@aws-amplify/notifications": "^2.0.7",
    "@aws-amplify/ui-react": "^6.0.6",
    "@cori-risi/cori.data.api": "github:ruralinnovation/cori.data.api",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "amazon-cognito-identity-js": "^6.3.8",
    "axios": "^1.6.2",
    "create-react-class": "^15.7.0",
    "d3": "^7.8.5",
    "d3-color": "^3.1.0",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.15.0",
    "next": "14.0.4",
    "plotly.js": "^2.27.1",
    "rc-slider": "^10.5.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-map-gl": "^7.1.6",
    "react-plotly.js": "^2.6.0",
    "src": "^1.1.2",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@aws-amplify/backend-cli": "^0.9.2",
    "@guanghechen/rollup-plugin-copy": "^5.0.12",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "aws-amplify": "^6.0.7",
    "esbuild": "^0.14.54",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "patch-package": "^8.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "styled-jsx": "^5.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^5.0.10",
    "worker-loader": "^3.0.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/modules/*.js",
      "!src/plugins/*.js",
      "!src/renv/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "build",
      "dist",
      "node_modules",
      "modules",
      "renv",
      "/build/",
      "/dist/",
      "/node_modules/",
      "/modules/",
      "/renv/",
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "testPathIgnorePatterns": [
      "build",
      "dist",
      "node_modules",
      "modules",
      "renv",
      "/build/",
      "/dist/",
      "/node_modules/",
      "/modules/",
      "/renv/",
      "<rootDir>/build",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/src/modules",
      "<rootDir>/src/renv",
      "<rootDir>\\build",
      "<rootDir>\\dist",
      "<rootDir>\\node_modules",
      "<rootDir>\\src\\modules",
      "<rootDir>\\src\\renv"
    ],
    "verbose": true
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "cross-env VITE_API_BACKEND=http://127.0.0.1:5174 VITE_COGNITO_REDIRECT_SIGNIN=http://localhost:5174 VITE_COGNITO_REDIRECT_SIGNOUT=http://localhost:5174/ vite build -m development",
    "build:docker": "docker build -f src/Dockerfile --build-arg gh_token=$GITHUB_PAT --progress=plain --target app -t docker.io/coriverse/shinyapps:amplify-frontend src",
    "deploy": "npm run deploy:docker && amplify publish",
    "deploy:docker": "npm run build && npm run build:docker && docker push docker.io/coriverse/shinyapps:amplify-frontend",
    "dev": "cross-env VITE_API_BACKEND=http://127.0.0.1:5174 vite",
    "dev:docker": "npm run build:docker && docker run -it --entrypoint Rscript -p 3838:3838 -p 5174:5174 --rm docker.io/coriverse/shinyapps:amplify-frontend /srv/shiny-server/frontend/.exec-shiny-app.R . 5174",
    "dev:linux": "npm run build:dev && npm run start & npm run dev",
    "dev:windows": "npm run build:dev && npm run start:windows && npm run dev",
    "docker": "npm run build && npm run build:docker && npm run start:docker",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:lint": "next lint",
    "next:start": "next start",
    "patch": "patch-package",
    "postinstall": "npm run patch && bash -ic \"if [[ $- == *i* ]]; then npm run postinstall:R; fi;\"",
    "postinstall:R": "echo \"Do you want to activate renv for this project? [Y, n]\"; read input;\n if [[ $input == \"Y\" || $input == \"y\" ]]; then\n   npm run postinstall:renv;\n else\n   npm run postinstall:r;\n fi",
    "postinstall:renv": "bash -lc \"if command -v Rscript > /dev/null; then echo 'setwd(\\\"src\\\"); source(\\\"renv/activate.R\\\");' > .Rprofile && Rscript -e 'renv::restore(clean = FALSE)'; fi\"",
    "postinstall:r": "bash -lc \"if command -v Rscript > /dev/null; then Rscript -e \\\"install.packages(c('remotes')); remotes::install_github('ruralinnovation/cori.apps'); remotes::install_github('ruralinnovation/coriverse');\\\"; fi\"",
    "start": "cd src && Rscript .exec-shiny-app.R . 5174",
    "start:docker": "docker run -p 3000:3000 --rm docker.io/coriverse/shinyapps:amplify-frontend",
    "start:windows": "start npm run dev:docker",
    "start:pg-graphql": "npm run start:pg-graphql -w src/modules/fullstack-graphql-book",
    "test": "jest --no-cache"
  },
  "workspaces": [
    "src/modules/fullstack-graphql-book"
  ]
}
